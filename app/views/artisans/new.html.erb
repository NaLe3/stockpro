<h1>Créer un nouvel artisan</h1>  

<%= form_with model: @artisan do |f| %>
  <%= f.label "Nom" %>
  <%= f.text_field :name %>
  <%= f.label "Prénom" %>
  <%= f.text_field :first_name %>
  <%= f.label "TJM" %>
  <%= f.number_field :tjm %>
  <%= f.label "Description" %>
  <%= f.text_field :desc%>
  <%= f.submit "Créer" %>
<% end %>